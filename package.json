{
  "name": "jlru",
  "version": "0.0.1",
  "description": "An in-memory key/value database based on the Least Recently Used algorithm",
  "main": "_dist/jlru.min.js",
  "bin": {},
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp makedist",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/jlru.git"
  },
  "keywords": [
    "es5",
    "lru",
    "least",
    "recently",
    "used",
    "in-memory",
    "cache",
    "key/value pair",
    "max-age"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/jlru/issues"
  },
  "homepage": "https://github.com/jclo/jlru",
  "dependencies": {},
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "del": "4.0.0",
    "eslint": "5.15.3",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "gulp": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.2",
    "gulp-header": "2.0.7",
    "gulp-replace": "1.0.0",
    "gulp-uglify": "3.0.2",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "nyc": "13.3.0",
    "opener": "1.5.1"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run makedist && npm run test && npm run check-coverage && npm run report"
    }
  }
}
